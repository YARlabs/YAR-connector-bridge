{
  "name": "validator",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "typechain": "typechain --target=ethers-v5 --out-dir ./src/typechain-types './src/abi/**/*.json'",
    "postinstall": "npm run typechain",
    "start": "ts-node ./src/main.ts",
    "docker": "cross-env MONGO_EXPRESS_AUTH=true npm run _docker",
    "docker_dev": "cross-env MODE=development MONGO_EXPRESS_AUTH=false npm run _docker",
    "_docker": "cd ../ && docker compose --env-file ../../.env.secrets build --no-cache && docker compose --env-file ../../.env.secrets up",
    "docker_clean": "docker compose rm"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typechain/ethers-v5": "^10.2.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@ethersproject/experimental": "^5.7.0",
    "bullmq": "^3.10.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "mongodb": "^5.1.0"
  }
}
