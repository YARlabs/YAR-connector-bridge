  "devDependencies": {
    "rimraf": "^4.4.1",
    "@typechain/ethers-v5": "^10.2.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "ethers_utils": "*",
    "configs": "*",
    "configs_secret": "*",
    "typechains": "*",
    "@ethersproject/experimental": "^5.7.0",
    "bullmq": "^3.10.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "mongodb": "^5.1.0"
  }
}
