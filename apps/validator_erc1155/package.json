{
  "name": "validator_erc1155",
  "version": "0.0.1",
  "scripts": {
    "typechain": "typechain --target=ethers-v5 --out-dir ./src/typechain-types './src/abi/**/*.json' || echo 'typechain fail'",
    "start": "ts-node ./src/main.ts",
    "clear": "rimraf ./src/typechain-types",
    "build_validator": "docker compose build",
    "build_validator_dev": "cross-env MODE=development MONGO_EXPRESS_AUTH=false MONGO_EXPRESS_USER=\"\" MONGO_EXPRESS_PASSWORD=\"\" docker compose build",
    "run_validator": "docker compose up",
    "run_validator_dev": "cross-env MODE=development MONGO_EXPRESS_AUTH=false MONGO_EXPRESS_USER=\"\" MONGO_EXPRESS_PASSWORD=\"\" npm run run_validator"
  
  },
  "keywords": [],
