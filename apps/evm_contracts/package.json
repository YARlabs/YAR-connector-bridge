{
  "name": "evm_contracts",
  "version": "0.0.1",
  "description": "",
  "main": "./hardhat.configs.js",
  "types": "./hardhat.configs.js",
  "scripts": {
    "export_abi": "hardhat export-abi",
    "export_deployments_addresses": "ts-node ./scripts/export_deployments_addresses.ts",
    "test_unit": "hardhat test --grep test_key_unit",
    "test_e2e": "hardhat test --grep test_key_e2e --network localhost",
    "node": "hardhat node --full-trace",
    "deploy": "npm run deploy:YarTest && npm run deploy:PolygonTest && npm run deploy:BinanceTest && npm run deploy:EthereumTest && npm run deploy:SkaleTest",
    "deploy:YarTest": "hardhat deploy --network yarTest --tags YarBridge",
    "deploy:PolygonTest": "hardhat deploy --network polygonTest --tags PolygonBridge",
    "deploy:BinanceTest": "hardhat deploy --network binanceTest --tags BinanceBridge",
    "deploy:EthereumTest": "hardhat deploy --network ethereumTest --tags EthereumBridge",
    "deploy:SkaleTest": "hardhat deploy --network chaosSkaleTest --tags SkaleBridge",
    "verify:YarTest": "hardhat --network yarTest etherscan-verify --api-key $YAR_EXPLORER_API_KEY",
    "verify:PolygonTest": "hardhat --network polygonTest etherscan-verify --api-key $POLYGON_EXPLORER_API_KEY",
    "verify:BinanceTest": "hardhat --network binanceTest etherscan-verify --api-key $BINANCE_EXPLORER_API_KEY",
    "verify:EthereumTest": "hardhat --network ethereumTest etherscan-verify --api-key $ETHEREUM_EXPLORER_API_KEY",
    "verify:SkaleTest": "hardhat --network chaosSkaleTest etherscan-verify --api-key $SKALE_EXPLORER_API_KEY"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ts-mocha": "^10.0.0",
    "rimraf": "^4.4.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.3",
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.8.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "axios": "^1.1.3",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.6",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.11.22",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-tracer": "^2.1.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1"
  },
  "dependencies": {
    "ethers_utils": "*",
    "configs": "*",
    "dotenv": "^16.0.3"
  }
}
