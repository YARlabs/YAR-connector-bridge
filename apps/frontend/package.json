{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm run typechain",
    "start": "cross-env GENERATE_SOURCEMAP=false react-scripts start",
    "build_frontend": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "typechain": "typechain --target=ethers-v5 --out-dir ./src/typechain-types './src/abi/**/*.json' || echo 'typechain fail'",
    "clear": "rimraf ./build && rimraf ./src/typechain-types",
    "frontend_dev": "npm run start"
  },
  "dependencies": {
    "@metamask/detect-provider": "*",
    "@typechain/ethers-v5": "^10.2.0",
    "@usedapp/core": "^1.2.2",
    "axios": "^1.2.2",
    "bignumber.js": "^9.1.1",
    "bootstrap": "^5.3.0-alpha1",
    "configs": "*",
    "ethers": "^5.7.2",
    "typechains": "*",
    "jquery": "^3.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^9.1.1",
    "typescript": "^4.9.4",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "rimraf": "^4.4.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "cross-env": "^7.0.3",
    "typechain": "^8.1.1",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "dotenv": "^16.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
